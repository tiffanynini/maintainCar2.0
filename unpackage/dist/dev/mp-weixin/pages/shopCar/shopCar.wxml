<view><view class="container"><view class="context"><view class="header"><checkbox value="cb" checked="{{pCheckStatus}}" data-event-opts="{{[['tap',[['checkChange',['$event']]]]]}}" bindtap="__e"></checkbox><text>自营店</text></view><block wx:for="{{skuData}}" wx:for-item="v" wx:for-index="i" wx:key="i"><view class="section"><view class="set1-top"><label class="checkbox"><checkbox value="cb" checked="{{v.checked}}" data-id="{{v.skuId}}" data-event-opts="{{[['tap',[['cCheckChange',[i]]]]]}}" bindtap="__e"></checkbox></label></view><view class="set1-middle"><image src="{{v.image}}"></image></view><view class="set1-bottom"><view><text>{{v.name}}</text><image src="../../static/images/del.png" data-event-opts="{{[['tap',[['del',['$0'],[[['skuData','',i,'skuId']]]]]]]}}" bindtap="__e"></image></view><text class="set1-p2">汽车类型：轿车  位置：全车</text><view><text>{{"单价："+v.price}}</text><view class="counter"><text class="reduce-add" style="{{'pointer-events:'+(isPointer)+';'}}" disabled="{{true}}" data-event-opts="{{[['tap',[['minusReduce',[i,'$0'],[[['skuData','',i,'skuId']]]]]]]}}" bindtap="__e">-</text><text class="amount">{{v.num}}</text><text class="reduce-add1" disabled="{{true}}" data-event-opts="{{[['tap',[['addReduce',[i,'$0'],[[['skuData','',i,'skuId']]]]]]]}}" bindtap="__e">+</text></view></view></view></view></block></view><view class="bu"></view><view class="footer"><text class="footer-font">总额：<text class="money" space="emsp">{{''+totalJi+''}}</text>元(不含运费)</text><navigator url="../confirmOrder/confirmOrder" open-type="navigate"><view class="footer-btn">去结算</view></navigator></view></view></view>