<view><view class="container"><block wx:for="{{skuData}}" wx:for-item="v" wx:for-index="i" wx:key="i"><view class="context"><view class="header"><checkbox value="cb" checked="{{v.pCheckStatus}}" data-event-opts="{{[['tap',[['checkChange',['$0'],[[['skuData','',i,'merchantId']]]]]]]}}" bindtap="__e"></checkbox><text>{{v.merchantName}}</text></view><block wx:for="{{v.content}}" wx:for-item="val" wx:for-index="index" wx:key="index"><view class="section"><view class="set1-top"><label class="checkbox"><checkbox value="cb" checked="{{val.checked}}" data-event-opts="{{[['tap',[['cCheckChange',['$0',index,'$1'],[[['skuData','',i,'merchantId']],[['skuData','',i],['content','',index,'skuId']]]]]]]}}" bindtap="__e"></checkbox></label></view><view class="set1-middle"><image src="{{val.image}}"></image></view><view class="set1-bottom"><view><text>{{val.name}}</text><image src="../../static/images/del.png" data-event-opts="{{[['tap',[['del',['$0'],[[['skuData','',i,'skuId']]]]]]]}}" bindtap="__e"></image></view><text class="set1-p2">汽车类型：轿车  位置：全车</text><view><text>{{"单价："+val.price}}</text><view class="counter"><text class="reduce-add" style="{{'pointer-events:'+(isPointer)+';'}}" disabled="{{true}}" data-event-opts="{{[['tap',[['minusReduce',[index,'$0'],[[['skuData','',i],['content','',index,'skuId']]]]]]]}}" bindtap="__e">-</text><text class="amount">{{val.num}}</text><text class="reduce-add1" disabled="{{true}}" data-event-opts="{{[['tap',[['addReduce',[index,'$0'],[[['skuData','',i],['content','',index,'skuId']]]]]]]}}" bindtap="__e">+</text></view></view></view></view></block></view></block><view class="bu"></view><view class="footer"><text class="footer-font">总额：<text class="money" space="emsp">{{''+totalJi+''}}</text>元(不含运费)</text><view data-event-opts="{{[['tap',[['jieSuan',['$event']]]]]}}" class="footer-btn" bindtap="__e">去结算</view></view></view></view>