  position: fixed;
  bottom: 0;
  left: 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 20rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
